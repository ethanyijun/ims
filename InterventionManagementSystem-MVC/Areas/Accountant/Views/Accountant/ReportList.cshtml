@model InterventionManagementSystem_MVC.Areas.Accountant.Models.ReportsViewModel

<h3>Report List</h3>
<hr />

<table id="table-clients" class="table table-striped table-bordered table-hover table-responsive" style="background-color: white">
    <thead>
        <tr>
            <th style="width: 70%">Name</th>

            <th style="width: 30%">Action</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model.ReportList)
        {
            <tr style="vertical-align: middle;">
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>


                <td>

                    @Html.ActionLink("Print", "PrintReport", "Accountant", new { Name = item.Name }, null)
                </td>
            </tr>
        }
    </tbody>
</table>

